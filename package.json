{
  "name": "symfony-routes",
  "version": "1.0.0",
  "type": "module",
  "description": "Use typed symfony route endpoints",
  "author": "Michael Cozzolino",
  "license": "ISC",
  "homepage": "https://github.com/michaelcozzolino/symfony-routes",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelcozzolino/symfony-routes.git"
  },
  "bugs": {
    "url": "https://github.com/michaelcozzolino/symfony-routes/issues"
  },
  "keywords": [
    "symfony",
    "routes",
    "frontend",
    "typed-routes"
  ],
  "main": "./dist/symfonyRoutes.umd.cjs",
  "module": "./dist/symfonyRoutes.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/symfonyRoutes.js",
      "require": "./dist/symfonyRoutes.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.1.1"
  }
}
